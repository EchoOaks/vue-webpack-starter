{
  "name": "vue-webpack-starter",
  "version": "1.0.0",
  "description": "Easily add vue to an existing page.",
  "main": "src/app/main.js",
  "scripts": {
    "serve": "webpack serve --node-env development --mode=development --env platform=development --progress",
    "serve:watch": "webpack serve --node-env development --mode=development --env platform=development --progress --watch",
    "build": "webpack --node-env production --mode=production --env platform=production --progress",
    "lint": "eslint ./src/app",
    "test:unit": "jest --passWithNoTests",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@vue/test-utils": "^1.1.3",
    "autoprefixer": "^10.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.0.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.14",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "eslint-webpack-plugin": "^2.5.2",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-postcss": "^9.0.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^27.0.1",
    "mini-css-extract-plugin": "^1.3.8",
    "postcss": "^8.2.4",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.0.0-beta.0"
